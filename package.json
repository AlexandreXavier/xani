    "github-slugger": "^2.0.0",
    "lucide-react": "^0.525.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.14",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/react": "^3.6.3",
