#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/prebuild-install@7.1.3/node_modules/prebuild-install/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/prebuild-install@7.1.3/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/prebuild-install@7.1.3/node_modules/prebuild-install/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/prebuild-install@7.1.3/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
