#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules/shelljs/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules/shelljs/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules/shelljs/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules/shelljs/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/shelljs@0.7.6/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../shelljs@0.7.6/node_modules/shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../../../../../shelljs@0.7.6/node_modules/shelljs/bin/shjs" "$@"
fi
