#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules/vite/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules/vite/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules/vite/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules/vite/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/vite@5.4.21_@types+node@24.9.1_lightningcss@1.30.2_terser@5.44.0/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
