#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/language-server/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/language-server/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/language-server/bin/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/language-server/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/@astrojs+language-server@2.15.5_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules:/Users/alexandrexavier/Documents/myWeb/alexandre/xani/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../../bin/nodeServer.js" "$@"
fi
